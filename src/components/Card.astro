---
const {
  firstName,
  lastName,
  role,
  email,
  phone,
  profileURL,
  imageURL,
  department,
} = Astro.props;
---

<div
  class="z-10 flex flex-col justify-between md:rounded-xl md:shadow-2xl transition-all duration-300 animate-fade-in md:h-auto h-screen w-screen md:max-w-md md:mb-6">
  <div class="flex flex-col items-center">
    <!-- Image Section -->
    <div
      class="items-center text-center align-middle bg-gradient-to-b from-smemint-400 to-smemint-500 dark:from-smenavy-500 dark:to-smenavy-600 w-full h-auto p-8 md:rounded-t-xl">
      <img
        loading="eager"
        src={"https://www.smesolicitors.co.uk/upload/team/" +
          profileURL +
          ".png"}
        alt={firstName + " " + lastName}
        title={firstName + " " + lastName}
        class="rounded-full w-60 h-60 mx-auto object-scale-down"
      />
    </div>

    <!-- Profile Section -->
    <div class="items-center text-center w-full p-8 grow h-full bg-white dark:bg-smenavy-800">
      <h1 class="text-5xl font-bold text-smenavy-600 dark:text-smemint-50 mb-2 font-generalSans">
        {firstName}
        {lastName}
      </h1>
      <h2
        class="uppercase tracking-wide text-smemint-800 dark:text-smemint-600 text-lg font-semibold font-inter">
        {role}
      </h2>

      <ul
        class="mt-5 rounded-2xl border-2 border-smemint-500 bg-smemint-100 dark:bg-smenavy-700 dark:border-smenavy-600 text-left">
        <div class="border-b-2 border-smemint-500 dark:border-smenavy-600 p-4">
          <li class="flex">
            <div
              class="rounded-full align-middle text-center bg-smenavy-500 dark:bg-smenavy-500 p-3 mr-3 w-fit h-fit">
              <svg
                class="w-6 h-6"
                viewBox="0 0 73 52"
